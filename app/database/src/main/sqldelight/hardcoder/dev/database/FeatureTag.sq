CREATE TABLE FeatureTag (
    id INTEGER AS Int NOT NULL PRIMARY KEY,
    name TEXT NOT NULL
);

insert:
INSERT INTO FeatureTag (id, name)
VALUES (?, ?);

deleteById:
DELETE FROM FeatureTag WHERE id = ?;

provideAllFeatureTags:
SELECT * FROM FeatureTag;

provideFeatureTagById:
SELECT * FROM FeatureTag WHERE id = ?;