CREATE TABLE HobbyTrack (
    id INTEGER AS Int NOT NULL PRIMARY KEY,
    name TEXT NOT NULL,
    iconResourceName TEXT NOT NULL
);

insert:
INSERT INTO HobbyTrack (id, name, iconResourceName)
VALUES (?, ?, ?);

update:
UPDATE HobbyTrack SET name = ?, iconResourceName = ? WHERE id = ?;

deleteById:
DELETE FROM HobbyTrack WHERE id = ?;

provideAllHobbyTracks:
SELECT * FROM HobbyTrack;

provideHobbyById:
SELECT * FROM HobbyTrack WHERE id = ?;