CREATE TABLE Hobby (
    id INTEGER AS Int NOT NULL PRIMARY KEY,
    name TEXT NOT NULL,
    iconId INTEGER NOT NULL
);

insert:
INSERT INTO Hobby (id, name, iconId)
VALUES (?, ?, ?);

update:
UPDATE Hobby SET name = ?, iconId = ? WHERE id = ?;

deleteById:
DELETE FROM Hobby WHERE id = ?;

provideAllHobbyTracks:
SELECT * FROM Hobby;

provideHobbyById:
SELECT * FROM Hobby WHERE id = ?;