import kotlinx.datetime.Instant;
import kotlin.Int;

CREATE TABLE IF NOT EXISTS AppPreference (
    id INTEGER AS Int NOT NULL PRIMARY KEY,
    firstLaunchTime INTEGER AS Instant,
    lastAppReviewRequestTime INTEGER AS Instant,
    lastEntranceDateTime INTEGER AS Instant NOT NULL
);

upsert:
INSERT OR REPLACE INTO AppPreference (id, firstLaunchTime, lastAppReviewRequestTime, lastEntranceDateTime)
VALUES (?, ?, ?, ?);

providePreferences:
SELECT * FROM AppPreference;
